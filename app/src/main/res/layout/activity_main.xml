<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <Button
        android:id="@+id/buttonPassInteger"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Pass integer value to 2nd activity" />

    <Button
        android:id="@+id/buttonPassChar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Pass Character value to 2nd activity" />

    <TextView
        android:id="@+id/textViewDouble"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Pass double value to 2nd activity" />
</LinearLayout>
